    <platform>all</platform>
	<forum></forum>
	<language></language>
	<website>https://github.com/brendoncrawford/socksipy-x</website>
	<source>https://github.com/brendoncrawford/socksipy-x</source>
	<email></email>
  </extension>
</addon>

